{
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": [
            false
        ],
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "eofline": false,
        "only-arrow-functions": false,
        "ordered-imports": false,
        "trailing-comma": false,
        "arrow-parens": false,
        "no-shadowed-variable": false
    },
    "rulesDirectory": []
}